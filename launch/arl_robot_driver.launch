<?xml version="1.0" encoding="utf-8"?>
<launch>

  <!-- Driver parameters -->
  <rosparam command="load" file="$(find arl_hw)/config/driver.yaml" />

  <!-- Load muscle configuration -->
  <rosparam command="load" file="$(find arl_controllers)/config/muscle_controller.yaml" />

  <!-- Robot Driver -->
  <node pkg="arl_hw" type="arl_driver_node" name="arl_driver_node" />

</launch>
